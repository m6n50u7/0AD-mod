<?xml version="1.0" encoding="utf-8"?>

<!-- Originally, This list encompasses resources and population. but we don't need these in this mod because resorces are stored inside the stores. not in the top panel. -->
<object size="0 0 50%-142 100%" name="resourceCounts">
	<repeat count="1000">
		<object name="resource[n]" size="0 0 0 100%" type="button" style="resourceCounter" tooltip_style="sessionToolTipBold">
			<object size="0 0 0 0" type="image" name="resource[n]_icon" ghost="true"/>
			<object size="0 0 0 0" type="text" style="resourceText"  name="resource[n]_count"/>
			<object size="0 0 0 0" type="text" style="gathererCounts" name="resource[n]_stats"/>
		</object>
	</repeat>
</object>
